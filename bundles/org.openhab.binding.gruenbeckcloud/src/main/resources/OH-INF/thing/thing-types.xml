	<bridge-type id="bridge">

		<label>Grünbeck Cloud Bridge</label>
		<description>Bridge representing the Grünbeck Cloud Account</description>

		<properties>
			<property name="vendor">Grünbeck</property>
		</properties>

		<representation-property>email</representation-property>

		<config-description>
			<parameter name="password" type="text">
				<required>true</required>
				<label>Password</label>
				<description>This is the Password required to log in to Grünbeck Cloud.</description>
			</parameter>
			<parameter name="refreshPeriod" type="integer" min="1" max="60">
				<required>true</required>
				<default>10</default>
				<label>Connect Refresh Period (seconds)</label>
				<description>
					This is the Frequency of the Polling Requests to the Grünbeck Cloud API. There are Limits
					to the Number
					of Requests that can be sent to the API. The more often you poll, the better
					Status Updates, at the Risk of running
					out of your Request Quota.
				</description>
			</parameter>
			<parameter name="email" type="text">
				<required>true</required>
				<label>Email Address</label>
				<description>This is the Account Email required to log in to Grünbeck Cloud</description>
			</parameter>
		</config-description>
	</bridge-type>

